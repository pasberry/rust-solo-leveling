// Arrays and builtin functions
let numbers = [1, 2, 3, 4, 5];

print("Length:");
print(len(numbers));

print("First:");
print(first(numbers));

print("Last:");
print(last(numbers));

print("Rest:");
print(rest(numbers));

print("Push:");
print(push(numbers, 6));

// Hash maps
let person = {
    "name": "Alice",
    "age": 30,
    "city": "San Francisco"
};

print(person["name"]);
print(person["age"]);
